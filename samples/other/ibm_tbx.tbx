                <langSet xml:lang="en">
                    <admin type="productSubset">Servers: eServer xSeries</admin>
                    <adminGrp>
                        <admin type="sourceIdentifier">Translation Services Center</admin>
                    </adminGrp>
                    <ntig>
                        <termGrp>
                            <term>navigation trail</term>
                            <termNote type="partOfSpeech">noun</termNote>
                        </termGrp>
                        <descrip type="context">The trail at the top of the content frame is your
                            navigation trail, sometimes called a branch.</descrip>
                    </ntig>
                </langSet>
                <langSet xml:lang="en">
                    <admin type="productSubset">Servers: eServer xSeries</admin>
                    <adminGrp>
                        <admin type="sourceIdentifier">Translation Services Center</admin>
                    </adminGrp>
                    <ntig>
                        <termGrp>
                            <term>branch</term>
                            <termNote type="partOfSpeech">noun</termNote>
                        </termGrp>
