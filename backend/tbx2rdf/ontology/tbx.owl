    </owl:DatatypeProperty>
    


    <!-- &tbx;morphologicalElement -->

    <owl:DatatypeProperty rdf:about="&tbx;morphologicalElement">
        <rdfs:comment xml:lang="en">indicates the individual morphemes that comprise the term.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;componentProperty"/>
