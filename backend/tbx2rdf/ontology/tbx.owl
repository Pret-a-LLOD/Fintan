<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY terms "http://purl.org/dc/terms/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY ontolex "http://www.w3.org/ns/lemon/ontolex#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY tbx "http://tbx2rdf.lider-project.eu/tbx#" >
]>


<rdf:RDF xmlns="&tbx;"
     xml:base="http://tbx2rdf.lider-project.eu/tbx"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ontolex="http://www.w3.org/ns/lemon/ontolex#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <owl:Ontology rdf:about="http://tbx2rdf.lider-project.eu/tbx">
        <rdfs:label xml:lang="en">TBX Ontology</rdfs:label>
        <rdfs:comment xml:lang="en">This ontology describes some of the elements defined in the TBX (TermBase eXchange) format (ISO 30042), a system for the exchange of terminological data.

This description is not complete, is not normative and is not supported by ISO or any other organization whatsoever. It is an initative of Universt&#228;t Bielefeld and Universidad Polit&#233;cnica de Madrid in the framework of the Lider project (http://www.lider-project.eu)</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&rdfs;label"/>
    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- &tbx;abbreviatedFormFor -->

    <owl:ObjectProperty rdf:about="&tbx;abbreviatedFormFor">
        <rdfs:comment xml:lang="en">A link used to identify a relation between a term that is an abbreviated form, and its full or expanded form. It may be coupled with a target attribute to point to the entry of the full or expanded form. For example, the term &quot;WWW&quot; could have a pointer to its full form &quot;World Wide Web&quot;.  The identification of a relation between an abbreviated term and its full form indicates which term in a terminological entry is the full form of the abbreviated term. In many languages, English abbreviations are commonly used, but the English full form may be avoided in favor of an equivalent full form in the language in question. In this case, the English abbreviation and the target language full form will both appear in the target language section of the entry.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:domain rdf:resource="&tbx;Term"/>
        <rdfs:range rdf:resource="&tbx;Term"/>
        <rdfs:subPropertyOf rdf:resource="&tbx;termRelation"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;antonymConcept -->

    <owl:ObjectProperty rdf:about="&tbx;antonymConcept">
        <rdfs:comment xml:lang="en">A concept that is opposite in meaning to the concept in the subject. 

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;antonymTerm -->

    <owl:ObjectProperty rdf:about="&tbx;antonymTerm">
        <rdfs:comment xml:lang="en">A term that designates a concept that is opposite in meaning to the subject term.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:domain rdf:resource="&tbx;Term"/>
        <rdfs:range rdf:resource="&tbx;Term"/>
        <rdfs:subPropertyOf rdf:resource="&tbx;termRelation"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;associatedConcept -->

    <owl:ObjectProperty rdf:about="&tbx;associatedConcept">
        <rdfs:comment xml:lang="en">A concept that exhibits a relation to the subject concept that can be established on the basis of non-hierarchical thematic connections.

Defintion originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;bidirectionalTranslation -->

    <owl:ObjectProperty rdf:about="&tbx;bidirectionalTranslation">
        <rdfs:comment xml:lang="en">A translation that is preferred in both directions, i.e. the term in the subject is a preferred translation of the term in the subject, and viceversa.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;translation"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;broaderConceptGeneric -->

    <owl:ObjectProperty rdf:about="&tbx;broaderConceptGeneric">
        <rdfs:comment xml:lang="en">A concept two or more levels of abstraction higher than subject concept. Generic relations between concepts are &quot;is-a&quot; relations. This means that the narrower concept &quot;is an&quot; instance of its broader generic concept. For example, a dog is a canine animal, and a canine animal is a mammal. Therefore, mammal is a broader generic concept for dog.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;broaderConceptPartitive -->

    <owl:ObjectProperty rdf:about="&tbx;broaderConceptPartitive">
        <rdfs:comment xml:lang="en">A concept two or more levels of abstraction higher than subject concept in a part-whole or meronymic hierarchical concept system. For example, a floor board is part of a house (broader partitive concept), but it can be more narrowly defined as being part of the floor (superordinate partitive concept), which in turn is part of the house. Partitive relations between concepts are &quot;has-a&quot; relations. This means that the broader partitive concept &quot;has&quot; its narrower concepts as parts of itself.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;classificationCode -->

    <owl:ObjectProperty rdf:about="&tbx;classificationCode">
        <rdfs:subPropertyOf rdf:resource="&tbx;usageInfo"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;conceptPosition -->

    <owl:ObjectProperty rdf:about="&tbx;conceptPosition">
        <rdfs:comment xml:lang="en">The position of a concept in a concept system.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;conceptRelation -->

    <owl:ObjectProperty rdf:about="&tbx;conceptRelation">
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;coordinateConceptGeneric -->

    <owl:ObjectProperty rdf:about="&tbx;coordinateConceptGeneric">
        <rdfs:comment xml:lang="en">A concept having the same nearest superordinate concept as the subject concept.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;coordinateConceptPartitive -->

    <owl:ObjectProperty rdf:about="&tbx;coordinateConceptPartitive">
        <rdfs:comment rdf:datatype="&xsd;string">A concept having the same nearest superordinate concept as the subject concept.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;corpusTrace -->

    <owl:ObjectProperty rdf:about="&tbx;corpusTrace">
        <rdfs:comment xml:lang="en">A notation indicating the location of an external contextual resource.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;crossReference"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&tbx;Term"/>
                    <rdf:Description rdf:about="&tbx;TerminologicalConcept"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- &tbx;crossReference -->

    <owl:ObjectProperty rdf:about="&tbx;crossReference">
        <rdfs:comment rdf:datatype="&xsd;string">A pointer to another related location, such as another entry or another term.</rdfs:comment>
        <rdfs:range rdf:resource="&tbx;XReference"/>
        <rdfs:subPropertyOf rdf:resource="&tbx;crossReference"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&tbx;Term"/>
                    <rdf:Description rdf:about="&tbx;TerminologicalConcept"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- &tbx;externalCrossReference -->

    <owl:ObjectProperty rdf:about="&tbx;externalCrossReference">
        <rdfs:comment xml:lang="en">A term or other terminological resource that is external to the terminological database or TBX document instance.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;crossReference"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&tbx;Term"/>
                    <rdf:Description rdf:about="&tbx;TerminologicalConcept"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- &tbx;falseFriend -->

    <owl:ObjectProperty rdf:about="&tbx;falseFriend">
        <rdfs:comment xml:lang="en">A term in another language that appears to be semantically or morphologically similar to the subject term in the entry, but that does not represent the same concept. For example, in French, the term r&#233;aliser means &quot;to accomplish&quot;, and therefore, it is a false friend of the English term realize (i.e. to become conscious of).

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf

</rdfs:comment>
        <rdfs:range rdf:resource="&tbx;Term"/>
        <rdfs:domain rdf:resource="&tbx;Term"/>
        <rdfs:subPropertyOf rdf:resource="&tbx;termRelation"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;homograph -->

    <owl:ObjectProperty rdf:about="&tbx;homograph">
        <rdfs:comment xml:lang="en">A word that is spelled like the subject term in the entry, but that has a different meaning, and origin. Homographs are terms that have the same written form, but are derived from different etymological origins, for example, a port for boats and a port for computer peripherals.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:domain rdf:resource="&tbx;Term"/>
        <rdfs:range rdf:resource="&tbx;Term"/>
        <rdfs:subPropertyOf rdf:resource="&tbx;termRelation"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;incommensurateTranslation -->

    <owl:ObjectProperty rdf:about="&tbx;incommensurateTranslation">
        <rdfs:comment xml:lang="en">The relation between the term in the subject and the term in the object as translations is imperfect and should be used with care. In this case, the concepts are not equal. A &lt;termNote type=&quot;transferComment&quot;&gt; should be used to provide information about the difference between the concepts.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;translation"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;indexHeading -->

    <owl:ObjectProperty rdf:about="&tbx;indexHeading">
        <rdfs:comment xml:lang="en">A term chosen to be used in an index to represent an item or a concept in a document.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;usageInfo"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;keyword -->

    <owl:ObjectProperty rdf:about="&tbx;keyword">
        <rdfs:comment xml:lang="en">A word or group of words, possibly in lexicographically standardized (lemmatized) form, taken out of the text of a document, which characterizes the text and enables its retrieval. For example, the terms data-category and terminological entry are keywords for this international standard. In a terminological database that contains the terminology of ISO 30042, they could be identified as keywords by using this element.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;usageInfo"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;monodirectionalTranslation -->

    <owl:ObjectProperty rdf:about="&tbx;monodirectionalTranslation">
        <rdfs:comment xml:lang="en">A translation that is only preferred going from the source language.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;translation"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;relatedConcept -->

    <owl:ObjectProperty rdf:about="&tbx;relatedConcept">
        <rdfs:comment xml:lang="en">A concept that has an associative relation to another concept, such as teacher and school.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;relatedConceptBroader -->

    <owl:ObjectProperty rdf:about="&tbx;relatedConceptBroader">
        <rdfs:comment xml:lang="en">A concept that is broader at some level with respect to a related or associated concept, but without comprising any reference to a generic or partitive system.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;relatedConceptNarrower -->

    <owl:ObjectProperty rdf:about="&tbx;relatedConceptNarrower">
        <rdfs:comment xml:lang="en">A concept that is narrower at some level with respect to a related or associated concept, but without comprising any reference to a generic or partitive system.

Definition taken from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;searchTerm -->

    <owl:ObjectProperty rdf:about="&tbx;searchTerm">
        <rdfs:comment xml:lang="en">A term entered in a term entry for purposes of retrieval.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;usageInfo"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;see -->

    <owl:ObjectProperty rdf:about="&tbx;see">
        <rdfs:comment xml:lang="en">An element that points to a location where additional information can be found.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;crossReference"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&tbx;Term"/>
                    <rdf:Description rdf:about="&tbx;TerminologicalConcept"/>
                    <rdf:Description rdf:about="&ontolex;LexicalEntry"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- &tbx;sequentiallyRelatedConcept -->

    <owl:ObjectProperty rdf:about="&tbx;sequentiallyRelatedConcept">
        <rdfs:comment xml:lang="en">A concept that has a relation to the subject concept based on spatial or temporal proximity. Temporal relations and spatial relations can be sequential.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;shortFormFor -->

    <owl:ObjectProperty rdf:about="&tbx;shortFormFor">
        <rdfs:comment xml:lang="en">A linking element used to identify a relation between the subject term when it is a short form, and its full or expanded form.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:domain rdf:resource="&tbx;Term"/>
        <rdfs:range rdf:resource="&tbx;Term"/>
        <rdfs:subPropertyOf rdf:resource="&tbx;termRelation"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;sortKey -->

    <owl:ObjectProperty rdf:about="&tbx;sortKey">
        <rdfs:comment xml:lang="en">A character string used for sorting and merging operations. A sort key can enable alphabetic or systematic access.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;usageInfo"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;spatiallyRelatedConcept -->

    <owl:ObjectProperty rdf:about="&tbx;spatiallyRelatedConcept">
        <rdfs:comment xml:lang="en">A concept that has a relation of dependence with the subject concept, referring to objects based on their relative physical positions.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;subordinateConceptGeneric -->

    <owl:ObjectProperty rdf:about="&tbx;subordinateConceptGeneric">
        <rdfs:comment xml:lang="en">A concept that has a generic relation with the subject concept, and a broader intension.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;subordinateConceptPartitive -->

    <owl:ObjectProperty rdf:about="&tbx;subordinateConceptPartitive">
        <rdfs:comment xml:lang="en">A concept that has a partitive relation with the subject concept, and represents one of the parts of the subject concept.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;superordinateConceptGeneric -->

    <owl:ObjectProperty rdf:about="&tbx;superordinateConceptGeneric">
        <rdfs:comment xml:lang="en">A concept that has a generic relation with the subject concept, and a narrower intension.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;superordinateConceptPartitive -->

    <owl:ObjectProperty rdf:about="&tbx;superordinateConceptPartitive">
        <rdfs:comment xml:lang="en">A concept that has a partitive relation with the subject concept, and represents the sum of the parts; a comprehensive concept.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;temporallyRelatedConcept -->

    <owl:ObjectProperty rdf:about="&tbx;temporallyRelatedConcept">
        <rdfs:comment xml:lang="en">A concept related to the subject concept through a sequential relation involving events in time.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptRelation"/>
        <rdfs:domain rdf:resource="&tbx;TerminologicalConcept"/>
        <rdfs:range rdf:resource="&tbx;TerminologicalConcept"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;termRelation -->

    <owl:ObjectProperty rdf:about="&tbx;termRelation">
        <rdfs:domain rdf:resource="&tbx;Term"/>
        <rdfs:range rdf:resource="&tbx;Term"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;thesaurusDescriptor -->

    <owl:ObjectProperty rdf:about="&tbx;thesaurusDescriptor">
        <rdfs:comment xml:lang="en">The term in a thesaurus that can be used to represent a concept in a terminological entry.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;usageInfo"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;transaction -->

    <owl:ObjectProperty rdf:about="&tbx;transaction">
        <rdfs:range rdf:resource="&tbx;Transaction"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ontolex;Form"/>
                    <rdf:Description rdf:about="&ontolex;LexicalEntry"/>
                    <rdf:Description rdf:about="&ontolex;LexicalSense"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- &tbx;translation -->

    <owl:ObjectProperty rdf:about="&tbx;translation">
        <rdfs:comment xml:lang="en">A property linking a term to one of its preferred translations.</rdfs:comment>
        <rdfs:domain rdf:resource="&tbx;Term"/>
        <rdfs:range rdf:resource="&tbx;Term"/>
        <rdfs:subPropertyOf rdf:resource="&tbx;termRelation"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;undeterminedTranslation -->

    <owl:ObjectProperty rdf:about="&tbx;undeterminedTranslation">
        <rdfs:comment xml:lang="en">A translation relation between two terms the scope and directionality of which is not determined.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;translation"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;usageInfo -->

    <owl:ObjectProperty rdf:about="&tbx;usageInfo">
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- &tbx;xAudio -->

    <owl:ObjectProperty rdf:about="&tbx;xAudio">
        <rdfs:comment xml:lang="en">External audio file.

