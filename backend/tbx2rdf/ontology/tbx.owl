    <!-- &tbx;subsetOwner -->

    <owl:DatatypeProperty rdf:about="&tbx;subsetOwner">
        <rdfs:comment xml:lang="en">The owner of the subset of terminological entries to which this entry belongs.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;logicalSubset"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&tbx;Term"/>
                    <rdf:Description rdf:about="&tbx;TerminologicalConcept"/>
                    <rdf:Description rdf:about="&ontolex;Lexicon"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- &tbx;syllabification -->

    <owl:DatatypeProperty rdf:about="&tbx;syllabification">
        <rdfs:comment xml:lang="en">indicates the individual syllables that comprise the term

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;componentProperty"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain rdf:resource="&ontolex;LexicalEntry"/>
    </owl:DatatypeProperty>
    


    <!-- &tbx;table -->

    <owl:DatatypeProperty rdf:about="&tbx;table">
        <rdfs:comment xml:lang="en">The name of a table that is used to describe a concept.

Definition originating from http://www.ttt.org/oscarstandards/tbx/tbx_oscar.pdf</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&tbx;conceptProperty"/>
