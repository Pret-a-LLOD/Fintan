                    <rdf:Description rdf:about="&tbx;TerminologicalConcept"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- &tbx;originatingInstitution -->

    <owl:DatatypeProperty rdf:about="&tbx;originatingInstitution">
