                    <rdf:Description rdf:about="&tbx;TerminologicalConcept"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- &tbx;originatingPerson -->

    <owl:DatatypeProperty rdf:about="&tbx;originatingPerson">
