                    <rdf:Description rdf:about="&tbx;Term"/>
                    <rdf:Description rdf:about="&tbx;TerminologicalConcept"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- &tbx;register -->

    <owl:DatatypeProperty rdf:about="&tbx;register">
